{
  "name": "censorbot",
  "version": "10.0.0",
  "description": "Anti-Swear bot for discord",
  "main": "./src/index.js",
  "scripts": {
    "s": "standard --fix -v",
    "commit": "git add . && echo \"Commit Message:\" && read msg && git commit -m \"$msg\"",
    "push": "git push",
    "jsdoc": "jsdoc ./ -r -c ./jsdoc.json --verbose -d ./docs",
    "log": "docker logs --tail=50 censorbot_prod_1 --follow",
    "buildsite": "cd website && rm -rf ./build && node ./src/PreBuild.js && tsc && webpack && cd ..",
    "site": "npm run buildsite && node ./website/server.js",
    "quicksite": "cd website && node ./src/PreBuild.js && cd .. && node ./website/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jpbberry/censorbot.git"
  },
  "author": "JPBBerry",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jpbberry/censorbot/issues"
  },
  "homepage": "https://github.com/jpbberry/censorbot#readme",
  "devDependencies": {
    "standard": "*",
    "jsdoc-baseline": "*",
    "typescript": "*",
    "webpack-cli": "*",
    "minify": "*"
  },
  "dependencies": {
    "@yaireo/tagify": "^3.22.0",
    "body-parser": "1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "datadog-metrics": "^0.8.1",
    "dotenv": "^8.2.0",
    "ejs": "3.0.1",
    "emoji-unicode-map": "1.1.10",
    "express": "4.17.1",
    "jsdoc": "^3.6.6",
    "moment": "2.24.0",
    "moment-duration-format": "2.3.2",
    "mongodb": "3.5.5",
    "node-fetch": "2.6.1",
    "os-utils": "0.0.14",
    "ws": "7.2.3"
  },
  "standard": {
    "ignore": [
      "website/views/*",
      "website/static/*",
      "docs/*",
      "node_modules/*"
    ],
    "globals": [
      "BigInt"
    ]
  },
  "eslintConfig": {
    "rules": {
      "func-call-spacing": "off",
      "multiline-ternary": "off"
    }
  }
}
